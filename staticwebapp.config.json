{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*", "/*.{css,js,json,png,jpg,svg,ico}"]
  },
  "globalHeaders": {
    "content-security-policy": "default-src 'self' 'unsafe-inline' 'unsafe-eval' https: data:",
    "x-frame-options": "SAMEORIGIN",
    "x-content-type-options": "nosniff"
  },
  "mimeTypes": {
    ".json": "application/json",
    ".js": "text/javascript",
    ".css": "text/css"
  }
}